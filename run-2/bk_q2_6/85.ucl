module StackModule {
  var idx: bv32;
  var s: bv32;
  var Stack: bv32;

  next {
    if (idx > 0bv32) {
      s' = (if (idx == 1bv32) then 0bv32 else s); //53
      idx' = (idx - 1bv32); //63
    }

  }
  invariant s_inv : ((s >= 0bv32) && (s <= Stack));
  invariant idx_inv : ((idx >= 0bv32) && (idx <= Stack));

  control  {
    v = bmc(3);
    check;
    print_results;
    v.print_cex();
  }
}
