module Module {
  var b1: boolean;
  var b2: boolean;
  var x: bv2;

  procedure step()
    modifies b1;
    modifies x;
    modifies b2;
  {
    b1 = true; //28
    x = 2bv2; //35
    b1 = false; //39
    b2 = true; //43
    if (x == 1bv2) {
      x = 2bv2; //59
    } else {
      x = 1bv2; //67
    }
    b2 = false; //72
  }

  next {
    call step();
  }
}
