module EventCounter {
  var count: integer;
  var temp: integer;

  init {
    count = 0; //22
    temp = 31; //26

  }
  next {
    temp' = (temp + 1); //35
    if (temp > 30) {
      count' = (count + 1); //48
    }

  }
  invariant temp_inv : (temp >= 0);
  invariant count_inv : (count >= 0);

  control  {
    v = bmc(3);
    check;
    print_results;
    v.print_cex();
  }
}
