module TrafficLight {
  var red: boolean;
  var green: boolean;

  init {
    red = true; //22
    green = false; //26

  }
  next {
    red' = !red; //34
    green' = !green; //40

  }
  invariant red_inv : !green;
  invariant green_inv : !red;

  control  {
    v = bmc(3);
    check;
    print_results;
    v.print_cex();
  }
}
